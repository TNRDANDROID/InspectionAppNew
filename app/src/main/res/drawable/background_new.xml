<?xml version="1.0" encoding="utf-8"?>


<layer-list xmlns:android="http://schemas.android.com/apk/res/android">



    <item>
        <shape android:shape="rectangle" android:tintMode="screen">
            <padding android:top="1dp" android:left="1dp" />
            <solid android:color="@color/colorAccentveryverylight"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>


    <item>
        <shape android:shape="rectangle">
            <padding android:top="5dp" android:left="5dp"/>
            <solid android:color="@color/colorAccentveryverylight"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <padding android:top="1dp" android:left="1dp" android:bottom="1dp" android:right="1dp"/>
            <solid android:color="@color/white"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <padding android:top="1dp" android:left="1dp" android:bottom="1dp" android:right="1dp"/>
            <solid android:color="#fafcfe"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>




</layer-list>